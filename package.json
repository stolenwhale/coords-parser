{
  "name": "coords-parser",
  "version": "1.0.0",
  "description": "Application for parsing coordinates from Google Geocoding API",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "build-win": "electron-packager . --overwrite --asar --platform=win32 --arch=ia32 --icon=src/assets/icons/icon.ico --prune=true --out=release-builds",
    "build-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "build-linux": "electron-packager . --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=release-builds"
  },
  "repository": "https://github.com/stolenwhale/coords-parser",
  "author": "StolenWhale",
  "license": "",
  "devDependencies": {
    "electron": "^15.5.5",
    "electron-packager": "^14.2.0"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "copy-to-clipboard": "^3.2.0"
  }
}
